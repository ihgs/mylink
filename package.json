  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:co": "react-scripts test --coverage --watchAll=false --coverageReporters html --coverageReporters text",
    "eject": "react-scripts eject",
    "build": "cross-env PUBLIC_URL=/mylink react-scripts build",
    "deploy": "npm run build && gh-pages -d build",
    "lint": "eslint src",
