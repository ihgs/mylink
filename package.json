{
  "name": "mylink",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "base-64": "^1.0.0",
    "daisyui": "^2.51.0",
    "js-file-download": "^0.4.12",
    "jszip": "^3.10.1",
